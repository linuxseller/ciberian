# 1. create array of 1000/6 elements and
# fill it with 1000-7 meme
# 2. print it

fn main() : void {
    i32 arr[143];
    i32 counter = 1000;
    i32 i = 0;
    while(counter>0){
        std.print i"\n";
        arr[i] = counter;
        counter=counter-7;
        i = i+1;
    }
    std.dprint i;
    std.dprint arr;
}
